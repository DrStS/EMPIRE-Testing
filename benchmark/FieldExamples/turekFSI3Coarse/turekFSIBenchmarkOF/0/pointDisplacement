FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    object      pointDisplacement;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    top
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    bottom
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    
    frontAndBack
    {
        type            empty;
    }
    
    inlet
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    
    outlet
    {
        type            fixedValue;
        value           uniform (0 0 0);
    } 

    cylinder
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    fsiWall
    {
       type            fixedValue;
       value           uniform (0 0 0);
    }
}

// ************************************************************************* //
